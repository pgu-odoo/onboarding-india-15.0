<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="expire_loyalty_program_config" model="ir.cron">
        <field name="name">Auto deactivated loyalty program</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="nextcall" eval="(DateTime.now() + timedelta(days=0)).strftime('%Y-%m-%d 18:30:00')"/>
        <field name="doall" eval="False"/>
        <field name="model_id" ref="model_loyalty_program_config"/>
        <field name="code">model.auto_deactivated_loyalty_program()</field>
        <field name="state">code</field>
    </record>
</odoo>
