<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="groups_institute_admin" model="res.groups">
        <field name="name">Institute/Admin</field>
    </record>
    <record id="only_active_modify" model="ir.rule">
    	<field name="name">Only Active Course Can be Modified</field>
    	 <field name="perm_read" eval="0"/>
    	 <field name="perm_write" eval="1"/>
    	 <field name="perm_create" eval="0"/>
    	 <field name="perm_unlink" eval="1"/>

    	 <field name="domain_force">[('active','=','True')]</field>
    	 <field name="model_id" ref="open_institute.model_institute_course"/>
        </record>
</odoo>
