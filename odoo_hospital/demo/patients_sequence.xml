<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
<!-- autom sequence Number for hospital.patients model -->
        <record id="seq_hospital_patients" model="ir.sequence">
            <field name="name">Patient sequence</field>
            <field name="code">patient.sequence</field>       
            <field name="prefix">P-</field>
            <field name="padding">5</field>
        </record>

        <record id="seq_hospital_appointment" model="ir.sequence">
            <field name="name">appointment sequence</field>
            <field name="code">appointment.sequence</field>       
            <field name="prefix">A-</field>
            <field name="padding">5</field>
        </record>
    </data>
</odoo> 